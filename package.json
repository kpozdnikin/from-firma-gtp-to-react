{
  "name": "figma-to-react",
  "version": "1.0.0",
  "description": "Convert Figma designs to React components",
  "main": "main.ts",
  "scripts": {
    "start": "ts-node -r dotenv/config main.ts",
    "build": "tsc && vite build",
    "dev": "ts-node-dev --respawn -r dotenv/config main.ts",
    "dev:react": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "type": "commonjs"
}
